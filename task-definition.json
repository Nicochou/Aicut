{
    "ipcMode": null,
    "executionRoleArn": "arn:aws:iam::999111338687:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
          "essential": true,
          "image": "aiserver",
          "name": "aiserver",
          "portMappings": [
              {
                  "containerPort": 8080,
                  "hostPort": 8080
              }
          ]
      },
      {
          "essential": true,
          "image": "client",
          "name": "client",
          "portMappings": [
              {
                  "containerPort": 3000,
                  "hostPort": 3000
              }
          ]
      },
      {
          "environment": [
              {
                  "name": "MYSQL_ROOT_PASSWORD",
                  "value": "root"
              }
          ],
          "essential": true,
          "image": "mysql:latest",
          "mountPoints": [
              {
                  "containerPath": "/var/lib/mysql",
                  "sourceVolume": "Db-Data"
              }
          ],
          "name": "db"
      },
      {
          "essential": true,
          "image": "server",
          "name": "server",
          "portMappings": [
              {
                  "containerPort": 8082,
                  "hostPort": 8082
              },
              {
                  "containerPort": 9229,
                  "hostPort": 9229
              },
              {
                  "containerPort": 9230,
                  "hostPort": 9230
              }
          ]
      }
  ],
    "placementConstraints": [],
    "memory": "512",
    "taskRoleArn": "",
    "family": "DeploymentContainer",
    "pidMode": null,
    "requiresCompatibilities": [
      "EC2"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "inferenceAccelerators": [],
    "proxyConfiguration": null,
    "volumes": [
      {
          "name": "Db-Data"
      }
  ]
}