{
  "containerDefinitions": [
      {
          "essential": true,
          "image": "999111338687.dkr.ecr.eu-west-3.amazonaws.com/ai-cut:client",
          "name": "client",
          "cpu": 600,
          "portMappings": [
              {
                  "containerPort": 3000,
                  "hostPort": 3000
              }
          ]
      },
      {
          "environment": [
              {
                  "name": "MYSQL_ROOT_PASSWORD",
                  "value": "root"
              }
          ],
          "essential": true,
          "image": "mysql:latest",
          "mountPoints": [
              {
                  "containerPath": "/var/lib/mysql",
                  "sourceVolume": "Db-Data"
              }
          ],
          "name": "db",
          "cpu": 600,
          "portMappings": [
            {
                "containerPort": 3306,
                "hostPort": 3306
            }
        ]
      },
      {
          "essential": true,
          "image": "999111338687.dkr.ecr.eu-west-3.amazonaws.com/ai-cut:server",
          "name": "server",
          "cpu": 600,
          "portMappings": [
              {
                  "containerPort": 8082,
                  "hostPort": 8082
              }
          ]
          
      }
  ],
  "volumes": [
      {
          "name": "Db-Data"
      }
  ],
  "inferenceAccelerators": [],
  "networkMode": "",
  "memory": "3250",
  "cpu": "1800",
  "executionRoleArn": "arn:aws:iam::999111338687:role/ecsTaskExecutionRole",
  "family": "DeploymentContainer",
  "placementConstraints": []
}